        <div class="modal fade" id="mkatt">
            <div class="modal-dialog modal-dialog-centered modal-lg">
                <div class="modal-content">
                    <!-- Modal Header -->
                    <div class="modal-header">
                        <h4 class="modal-title">Record Attendance</h4>
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                    </div>
                    <!-- Modal body -->
                    <div class="modal-body">
                        <div class="container">
                            <div class="row" style="padding:10px 50px;">
                                <!--ToggleSwitch--> 
                                <div style="padding:1px 16px;">
                                    <label class="switch">
                                        <input type="checkbox" class="chbx1" id="cswitch1" onclick="stoggle('cswitch1')">
                                        <span class="slider round"></span>
                                    </label>
                                </div>
                                <div style="padding:1px 16px;">
                                    <label class="switch">
                                        <input type="checkbox" class="disable" id="cswitch2" onclick="udstoggle(this)">
                                        <span class="slider1 round"></span>
                                    </label>
